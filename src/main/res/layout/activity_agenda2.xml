<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Agenda2Activity"
        android:id="@+id/background2">


    <TextView
            android:text="THURSDAY"
            android:layout_width="275dp"
            android:layout_height="60dp" android:id="@+id/thursday"
            android:textSize="30dp" android:textStyle="bold"
            android:gravity="center" app:layout_constraintTop_toTopOf="@id/background2"
            android:layout_marginTop="48dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="269dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line1thursday"
            android:hint="Things to do"
            app:layout_constraintTop_toBottomOf="@id/thursday"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="269dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line2thursday"
            android:hint="Things to do"
            app:layout_constraintTop_toBottomOf="@id/line1thursday"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="269dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line3thursday"
            android:hint="Things to do"
            app:layout_constraintTop_toBottomOf="@id/line2thursday"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="269dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line4thursday"
            android:hint="Things to do"
            app:layout_constraintTop_toBottomOf="@+id/line3thursday" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="4dp" app:layout_constraintStart_toStartOf="parent"/>



    <TextView
            android:text="FRIDAY"
            android:layout_width="275dp"
            android:layout_height="60dp" android:id="@+id/friday"
            android:textSize="30dp" android:textStyle="bold" android:gravity="center"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/line4thursday"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="269dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line1friday"
            android:hint="Things to do"
            app:layout_constraintTop_toBottomOf="@+id/friday" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="269dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line2friday"
            android:hint="Things to do"
            app:layout_constraintTop_toBottomOf="@+id/line1friday" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="269dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line3friday"
            android:hint="Things to do"
            app:layout_constraintTop_toBottomOf="@+id/line2friday" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="269dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line4friday"
            android:hint="Things to do"
            app:layout_constraintTop_toBottomOf="@+id/line3friday" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />


    <TextView
            android:text="SATURDAY"
            android:layout_width="230dp"
            android:layout_height="60dp" android:id="@+id/saturday"
            android:textSize="30dp" android:textStyle="bold"
            android:gravity="center"
            app:layout_constraintTop_toBottomOf="@+id/line4friday"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="@+id/line1saturday"
            app:layout_constraintStart_toStartOf="@+id/line1saturday"/>
    <EditText
            android:layout_width="160dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line1saturday"
            android:hint="Things to do"
            app:layout_constraintTop_toBottomOf="@+id/saturday"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"/>
    <EditText
            android:layout_width="160dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line2saturday"
            android:hint="Things to do"
            app:layout_constraintStart_toStartOf="@+id/line1saturday"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintTop_toTopOf="@+id/line2sunday"/>
    <TextView
            android:text="SUNDAY"
            android:layout_width="180dp"
            android:layout_height="60dp"
            android:textSize="30dp"
            android:id="@+id/sunday"
            android:textStyle="bold"
            android:gravity="center"
            app:layout_constraintTop_toBottomOf="@+id/line4friday"
            android:layout_marginTop="20dp" app:layout_constraintEnd_toEndOf="@+id/line1sunday"
            app:layout_constraintStart_toStartOf="@+id/line1sunday"/>
    <EditText
            android:layout_width="160dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line1sunday"
            android:hint="Things to do"
            app:layout_constraintTop_toBottomOf="@+id/sunday"
            app:layout_constraintTop_toTopOf="@+id/line1saturday"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"/>
    <EditText
            android:layout_width="160dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/line2sunday"
            android:hint="Things to do"
            app:layout_constraintEnd_toEndOf="@+id/line1sunday"
            app:layout_constraintTop_toBottomOf="@+id/line1sunday"/>
</androidx.drawerlayout.widget.DrawerLayout>